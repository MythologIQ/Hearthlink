{
  "plugin_id": "superclaude",
  "name": "SuperClaude Advanced AI",
  "version": "1.0.0",
  "description": "Enhanced Claude integration with advanced reasoning capabilities and tool integration",
  "type": "ai_assistant",
  "author": "Hearthlink Core Team",
  "capabilities": [
    "advanced_reasoning",
    "tool_integration",
    "context_awareness",
    "claude_code_cli",
    "claude_api_fallback",
    "conversation_memory",
    "multi_modal_analysis"
  ],
  "reasoning_modes": [
    {
      "mode": "fast",
      "description": "Quick responses for simple queries",
      "context_limit": 4096,
      "temperature": 0.3
    },
    {
      "mode": "balanced",
      "description": "Optimal balance of speed and quality",
      "context_limit": 8192,
      "temperature": 0.7
    },
    {
      "mode": "deep",
      "description": "Thorough analysis and reasoning",
      "context_limit": 16384,
      "temperature": 0.5
    },
    {
      "mode": "creative",
      "description": "Creative and exploratory responses",
      "context_limit": 8192,
      "temperature": 0.9
    }
  ],
  "tools": [
    {
      "tool_id": "code_interpreter",
      "name": "Code Interpreter",
      "description": "Execute and analyze code snippets",
      "enabled": true
    },
    {
      "tool_id": "file_manager",
      "name": "File Manager", 
      "description": "Read, write, and analyze files",
      "enabled": true
    },
    {
      "tool_id": "web_search",
      "name": "Web Search",
      "description": "Search and analyze web content",
      "enabled": false
    },
    {
      "tool_id": "data_analysis",
      "name": "Data Analysis",
      "description": "Analyze data and generate insights",
      "enabled": true
    }
  ],
  "backends": {
    "primary": "claude_code_cli",
    "fallback": "claude_api"
  },
  "security": {
    "sandbox_enabled": true,
    "audit_logging": true,
    "token_tracking": true,
    "rate_limiting": {
      "requests_per_minute": 30,
      "tokens_per_hour": 10000
    }
  },
  "integration": {
    "hearthlink_ecosystem": true,
    "alice_insights": true,
    "mimic_personas": true,
    "vault_storage": true
  }
}