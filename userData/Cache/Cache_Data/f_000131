        body: JSON.stringify(settings.apis)
      });
      const result = await response.json();
      setTestResults(prev => ({
        ...prev,
        [service]: result.success ? 'success' : 'failed',
        [`${service}_details`]: result.details || result.error
      }));
    } catch (err) {
      setTestResults(prev => ({
