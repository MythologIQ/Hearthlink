  const loadSettings = async () => {
    try {
      const response = await fetch('http://localhost:8003/api/settings');
      if (response.ok) {
        const loadedSettings = await response.json();
        setSettings(prev => ({
          ...prev,
          ...loadedSettings
        }));
      }
    } catch (err) {
      console.warn('Failed to load settings:', err);
      // Use defaults
    }
