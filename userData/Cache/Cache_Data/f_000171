  const saveSettings = async () => {
    setSaving(true);
    setError('');
    setSuccessMessage('');
    try {
      console.log('SettingsManager - Saving settings:', settings);
      const response = await fetch('http://localhost:8003/api/settings', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(settings)
      });
      console.log('SettingsManager - Save response status:', response.status);
      if (response.ok) {
        const result = await response.json();
        console.log('SettingsManager - Save result:', result);
        onSettingsChange(settings);
        setSuccessMessage('âœ… Settings saved successfully!');
        setSaving(false);

        // Clear success message after 3 seconds
        setTimeout(() => setSuccessMessage(''), 3000);
      } else {
        const errorResult = await response.json().catch(() => ({
          message: 'Unknown error'
        }));
        console.error('SettingsManager - Save failed:', response.status, errorResult);
        throw new Error(`Failed to save settings: ${response.status} - ${errorResult.message || 'Unknown error'}`);
      }
    } catch (err) {
      console.error('SettingsManager - Save error:', err);
      setError(`Save failed: ${err.message}`);
      setSaving(false);
    }
  };
