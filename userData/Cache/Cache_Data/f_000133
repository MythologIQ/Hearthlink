        setTestResults(prev => ({
          ...prev,
          'local-llm': 'success',
          'local-llm_details': `Service running: ${data.version || 'Unknown version'}`
        }));
      } else {
        throw new Error(`HTTP ${response.status}: ${response.statusText}`);
      }
    } catch (error) {
      setTestResults(prev => ({
        ...prev,
        'local-llm': 'failed',
        'local-llm_details': `Failed to start service: ${error.message}`
      }));
    }
  };
