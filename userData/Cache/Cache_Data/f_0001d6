  // Modal handlers

  const closeModals = () => {
    setShowConnectionModal(false);
    setEditingConnection(null);
    setEditingApi(null);
    setConnectionForm({
      name: '',
      type: 'mcp_connector',
      description: '',
      plugin_id: '',
      mcp_server: ''
    });
