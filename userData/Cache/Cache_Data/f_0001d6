  // Modal state
  const [showConnectionModal, setShowConnectionModal] = (0,react__WEBPACK_IMPORTED_MODULE_0__.useState)(false);
  const [editingConnection, setEditingConnection] = (0,react__WEBPACK_IMPORTED_MODULE_0__.useState)(null);
  const [editingApi, setEditingApi] = (0,react__WEBPACK_IMPORTED_MODULE_0__.useState)(null);

  // Form state
