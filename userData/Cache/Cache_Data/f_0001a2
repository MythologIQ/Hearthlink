      }
    } catch (error) {
      console.warn('Core API services not available, using fallback data:', error);
      updateApiStatus('services', false);
      addOrchestrationLog('Using fallback service data (Core API unavailable)', 'warning');

      // Fallback to static data
      setServices([{
        id: 'vault',
        name: 'Vault Storage',
        status: 'operational',
        health: 98,
